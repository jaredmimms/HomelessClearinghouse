AAA,GAAO,KAAP;AAA2B,MAAA,KAAA,CAAA,OAAA,GAAO,KAAP;KAAnE;AAOH,GATT,EAOC,UAPD,C,iCAYI,EAFO,KAAA,CAAA,O,kBAAX,YAAA,CAAmE,KAAnE,EAAA,WAAA,EAAoC,2BAApC,C,iCAEI,EADO,KAAA,CAAA,OAAA,IAAW,KAAA,CAAA,M,kBAAtB,YAAA,CAAqE,KAArE,EAAA,WAAA,EAA4C,qBAA5C,C,iCACI,CAlEN,C,mBAmEA,YAAA,CAEM,KAFN,EAEM,WAFN,EAEM,CADJ,YAAA,CAAgC,IAAhC,EAAgC,IAAhC,EAAI,UAAK,gBAAA,CAAG,MAAA,CAAA,EAAH,CAAL,GAAa,YAAjB,EAA2B,CAA3B,CACI,CAFN,C,CAmBI,EAfO,QAAA,CAAA,I,kBAAX,YAAA,CAEM,KAFN,EAEM,WAFN,EAEM,CADJ,YAAA,CAA4C,QAA5C,EAA4C;AAA/B,IAAA,OAAK,EAAA,MAAA,CAAA,EAAA,CAAA,KAAA,MAAA,CAAA,EAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,OAAA,IAAA,QAAA,CAAA,OAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAA0B,GAA5C,EAA6B,QAA7B,CACI,CAFN,C,iCAeI,EAXO,QAAA,CAAA,a,kBAAX,YAAA,CAIM,KAJN,EAIM,WAJN,EAIM,CAHJ,YAAA,CAEc,sBAFd,EAEc;AAFM,IAAA,EAAE,EAAA,YAAc,QAAA,CAAA,aAAA,CAAc;AAEpC,GAFd,E;qBACE;AAAA,aAAuC,CAAvC,WAAuC,CAAvC;AAAA,K;;GADF,E,CAAA,E,MAAA,CAGI,CAJN,C,iCAWI,EALO,QAAA,CAAA,S,kBAAX,YAAA,CAIM,KAJN,EAIM,WAJN,EAIM,CAHJ,YAAA,CAEc,sBAFd,EAEc;AAFM,IAAA,EAAE,EAAA,YAAc,QAAA,CAAA,SAAA,CAAU;AAEhC,GAFd,E;qBACE;AAAA,aAA+B,CAA/B,WAA+B,CAA/B;AAAA,K;;GADF,E,CAAA,E,MAAA,CAGI,CAJN,C,iCAKI,CA5FN,C","sourcesContent":["<template>\n  <div id=\"offer-page\">\n    <p>\n      Email <a href=\"mailto: jaredmimms@gmail.com\">jaredmimms@gmail.com</a> with\n      id number of offering and justification for services to acquire offer for\n      (a) homeless individual(s).\n    </p>\n    <div v-if=\"offer\">\n      <h1>Offer: {{ id }}</h1>\n      <div class=\"title\">{{ offer.title }}</div>\n\n      <div class=\"content\">{{ offer.content }}</div>\n      <div v-if=\"updated\">\n        <label for=\"title\">Title</label>\n        <input\n          type=\"text\"\n          v-model=\"offer.title\"\n          id=\"title\"\n          v-on:blur=\"validate\"\n        />\n        <div v-if=\"errors && errors.title\" class=\"error\">\n          <li v-for=\"(item, index) in errors.title\" v-bind:key=\"index\">\n            {{ item }}\n          </li>\n        </div>\n\n        <label for=\"featured\" class=\"form-checkbox-label\">\n          <input type=\"checkbox\" v-model=\"offer.featured\" id=\"featured\" />\n          Featured?\n        </label>\n\n        <label for=\"content\">Content</label>\n        <textarea\n          v-model=\"offer.content\"\n          id=\"content\"\n          v-on:blur=\"validate\"\n        ></textarea>\n        <div v-if=\"errors && errors.content\" class=\"error\">\n          <li v-for=\"(item, index) in errors.content\" v-bind:key=\"index\">\n            {{ item }}\n          </li>\n        </div>\n\n        <label for=\"tags\">Tags (separated by comma)</label>\n        <input\n          type=\"text\"\n          v-model=\"offer.tags\"\n          id=\"tags\"\n          v-on:blur=\"validate\"\n        />\n        <div v-if=\"errors && errors.tags\" class=\"error\">\n          <li v-for=\"(item, index) in errors.tags\" v-bind:key=\"index\">\n            {{ item }}\n          </li>\n        </div>\n      </div>\n\n      <button v-if=\"!updated && user\" v-on:click=\"updated = true\">\n        Update\n      </button>\n      <button v-if=\"updated && user\" v-on:click=\"update\">Update</button>\n      <button\n        v-if=\"updated\"\n        v-on:click=\"\n          updated = false;\n          success = false;\n          failure = false;\n        \"\n      >\n        Cancel\n      </button>\n      <div v-if=\"success\" class=\"success\">Post successfully updated</div>\n      <div v-if=\"failure && errors\" class=\"error\">Error updating post</div>\n    </div>\n    <div v-else>\n      <h2>Post {{ id }} not found</h2>\n    </div>\n\n    <div v-if=\"user\">\n      <button v-on:click=\"deleted\">Delete</button>\n    </div>\n\n    <div v-if=\"previousOffer\">\n      <router-link v-bind:to=\"'/offer/' + previousOffer.id\">\n        <button id=\"previous\">Previous</button>\n      </router-link>\n    </div>\n\n    <div v-if=\"nextOffer\">\n      <router-link v-bind:to=\"'/offer/' + nextOffer.id\">\n        <button id=\"next\">Next</button>\n      </router-link>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { axios } from \"@/common/app.js\";\nimport Validator from \"validatorjs\";\nimport { router } from \"@/common/router.js\";\n\nexport default {\n  props: {\n    id: {\n      type: String,\n    },\n    offers: {\n      type: Array,\n      default: null,\n    },\n  },\n  data() {\n    return {\n      updated: false,\n      failure: false,\n      success: false,\n      errors: [],\n    };\n  },\n  methods: {\n    update() {\n      if (this.validate()) {\n        var definedOffer = this.offer;\n        if (definedOffer.featured) {\n          definedOffer.featured = 1;\n        } else {\n          definedOffer.featured = 0;\n        }\n        axios\n          .put(\"/offerput/\" + this.offer.id, { offer: definedOffer })\n          .then((response) => {\n            if (response.data && response.data.errors) {\n              this.failure = true;\n              this.success = false;\n              // continue from here with errors\n              this.errors = response.data.errors;\n            } else {\n              this.errors = null;\n              this.success = true;\n              this.failure = false;\n              this.$emit(\"update-offers\");\n            }\n          });\n      }\n    },\n    validate() {\n      let validator = new Validator(this.offer, {\n        title: \"required|between:3,100\",\n        tags: \"required|between:3,100\",\n        content: \"required|min:100\",\n      });\n\n      if (validator.fails()) {\n        this.errors = validator.errors.all();\n      } else {\n        this.errors = null;\n      }\n\n      return validator.passes();\n    },\n    deleted() {\n      axios.delete(\"/offerdelete/\" + this.offer.id).then((response) => {\n        if (response.data && response.data.errors) {\n          this.failure = true;\n          this.success = false;\n          // continue from here with errors\n          this.errors = response.data.errors;\n        } else {\n          this.errors = null;\n          this.success = true;\n          this.failure = false;\n          this.$emit(\"update-offers\");\n          router.push(\"/offers\");\n        }\n      });\n    },\n  },\n  computed: {\n    user() {\n      return this.$store.state.user;\n    },\n    nextOffer() {\n      var offered;\n      if (this.offers && this.offer) {\n        var id = this.offer.id;\n        this.offers.forEach(function (offer, index, offers) {\n          if (id == offer.id) {\n            if (offers[index + 1]) {\n              offered = offers[index + 1];\n            } else if (offers.length > 1) {\n              offered = offers[0];\n            }\n          }\n        });\n      }\n      return offered;\n    },\n    previousOffer() {\n      var offered;\n      if (this.offers && this.offer) {\n        var id = this.offer.id;\n        this.offers.forEach(function (offer, index, offers) {\n          if (id == offer.id) {\n            if (offers[index - 1]) {\n              offered = offers[index - 1];\n            } else if (offers.length > 1) {\n              offered = offers[offers.length - 1];\n            }\n          }\n        });\n      }\n      return offered;\n    },\n    offer() {\n      if (this.offers) {\n        var offer;\n        var id = this.id;\n        this.offers.forEach(function (offered) {\n          if (offered.id == id) {\n            offer = offered;\n          }\n        });\n        if (offer) {\n          if (Number(offer.featured) == 1) {\n            offer.featured = true;\n          } else {\n            offer.featured = false;\n          }\n        }\n        return offer;\n      } else {\n        return null;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.error {\n  color: red;\n}\n\n.success {\n  color: green;\n}\n\n#inputs {\n  text-align: left;\n}\n\ninput,\nlabel,\ntextarea,\nbutton {\n  display: block;\n  float: left;\n  clear: both;\n}\n\n#product-page {\n  border: 1px solid var(--silver);\n  text-align: center;\n  padding: 15px;\n  margin-left: auto;\n  margin-right: auto;\n  width: 30%;\n  min-width: 300px;\n}\n\n.title {\n  height: 50px;\n  font-size: 2rem;\n  margin: 5px 0 10px 0;\n  vertical-align: baseline;\n}\n\n.content {\n  margin: auto;\n  text-align: left;\n  font-style: italic;\n  line-height: 1.5;\n}\n</style>"],"sourceRoot":""}]}