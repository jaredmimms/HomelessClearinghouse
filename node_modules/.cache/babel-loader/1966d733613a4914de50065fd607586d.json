AL,CAAY,GAAZ,CAAgB,UAAU,KAAV,EAAiB;AAChD,cAAI,KAAK,CAAC,UAAV,EAAsB;AACpB,mBAAO,KAAK,CAAC,UAAN,CAAiB,KAAjB,CAAuB,GAAvB,CAAP;AACF;AACD,SAJgB,CAAjB;AAKA,YAAI,gBAAe,GAAI,GAAG,MAAH,CAAU,KAAV,CAAgB,EAAhB,EAAoB,UAApB,CAAvB,CANe,CAQf;;AACA,YAAI,IAAG,GAAI,mBAAI,IAAI,GAAJ,CAAQ,gBAAR,CAAJ,EAA+B,IAA/B,EAAX;;AACA,YAAI,OAAM,GAAI,KAAK,MAAnB;AACA,QAAA,IAAI,CAAC,OAAL,CAAa,UAAU,MAAV,EAAkB;AAC7B,cAAI,SAAQ,GAAI,EAAhB;AACA,UAAA,OAAO,CAAC,OAAR,CAAgB,UAAU,OAAV,EAAmB;AACjC,gBAAI,OAAO,CAAC,UAAZ,EAAwB;AACtB,kBAAI,SAAQ,GAAI,OAAO,CAAC,UAAR,CAAmB,KAAnB,CAAyB,GAAzB,CAAhB;AACA,cAAA,SAAS,CAAC,OAAV,CAAkB,UAAU,KAAV,EAAiB;AACjC,oBAAI,KAAI,IAAK,MAAb,EAAqB;AACnB,kBAAA,SAAS,CAAC,IAAV,CAAe,OAAf;AACF;AACD,eAJD;AAKF;AACD,WATD;;AAUA,cAAI,SAAS,CAAC,CAAD,CAAb,EAAkB;AAChB,YAAA,OAAO,CAAC,IAAR,CAAa;AAAE,cAAA,IAAI,EAAE,MAAR;AAAgB,cAAA,UAAU,EAAE;AAA5B,aAAb;AACF;AACD,SAfD;AAgBF;;AACA,aAAO,OAAP;AACD;AAhCO;AAvBG,CAAf","sourcesContent":["<template>\n  <div id=\"categories-page\">\n    <h2>Tags</h2>\n\n    <button v-if=\"offerArray[0]\" v-on:click=\"cancel\" class=\"btn btn-primary\">\n      Cancel\n    </button>\n\n    <show-offer\n      v-for=\"offer in offerArray\"\n      v-bind:key=\"offer.id\"\n      v-bind:offer=\"offer\"\n    ></show-offer>\n\n    <ul v-if=\"!offerArray[0]\" class=\"clean-list\">\n      <li v-for=\"(category, id) in categories\" v-bind:key=\"id\">\n        <button\n          v-on:click=\"showCategory(category.offerArray)\"\n          class=\"btn btn-primary\"\n        >\n          {{ category.item }}\n        </button>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nimport ShowOffer from \"@/components/ShowOffer.vue\";\n\nexport default {\n  components: {\n    \"show-offer\": ShowOffer,\n  },\n  props: {\n    offers: {\n      type: Array,\n      default: null,\n    },\n  },\n  data() {\n    return {\n      offerArray: [],\n    };\n  },\n  methods: {\n    showCategory(offerArray) {\n      this.offerArray = offerArray;\n    },\n    cancel() {\n      this.offerArray = [];\n    },\n  },\n  computed: {\n    categories() {\n      var offered = [];\n      if (this.offers) {\n        let categories = this.offers.map(function (offer) {\n          if (offer.categories) {\n            return offer.categories.split(\",\");\n          }\n        });\n        let mergedCategories = [].concat.apply([], categories);\n\n        // Return unique, sorted categories\n        let item = [...new Set(mergedCategories)].sort();\n        var offerss = this.offers;\n        item.forEach(function (itemed) {\n          var allOffers = [];\n          offerss.forEach(function (offered) {\n            if (offered.categories) {\n              var itemArray = offered.categories.split(\",\");\n              itemArray.forEach(function (itemd) {\n                if (itemd == itemed) {\n                  allOffers.push(offered);\n                }\n              });\n            }\n          });\n          if (allOffers[0]) {\n            offered.push({ item: itemed, offerArray: allOffers });\n          }\n        });\n      }\n      return offered;\n    },\n  },\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":""}]}