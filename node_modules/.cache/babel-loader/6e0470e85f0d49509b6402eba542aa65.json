AA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,KAAA,CAAA,OAAA,GAAO,IAAT;AAAA,KAAA;AAAiC,GAAnE,EAAoD,QAApD,C,qCAcI,EAbU,KAAA,CAAA,O,kBAAd,YAAA,CAA0D,QAA1D,EAA0D;UAAA;AAA9B,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,MAAA,IAAA,QAAA,CAAA,MAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAAyB,GAA1D,EAA2C,QAA3C,C,qCAaI,EAXI,KAAA,CAAA,O,kBADR,YAAA,CASS,QATT,EASS;UAAA;AAPF,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA,EAAA;AAAa,MAAA,KAAA,CAAA,OAAA,GAAO,KAAP;AAA2B,MAAA,KAAA,CAAA,OAAA,GAAO,KAAP;AAA2B,MAAA,KAAA,CAAA,OAAA,GAAO,KAAP;KAAnE;AAOH,GATT,EAOC,UAPD,C,qCAYI,EAFO,KAAA,CAAA,O,kBAAX,YAAA,CAAmE,KAAnE,EAAA,WAAA,EAAoC,2BAApC,C,qCAEI,EADO,KAAA,CAAA,OAAA,IAAW,KAAA,CAAA,M,kBAAtB,YAAA,CAAqE,KAArE,EAAA,WAAA,EAA4C,qBAA5C,C,qCACI,CA5CN,C,mBA6CA,YAAA,CAEM,KAFN,EAEM,WAFN,EAEM,CADJ,YAAA,CAAgC,IAAhC,EAAgC,IAAhC,EAAI,UAAK,gBAAA,CAAG,MAAA,CAAA,EAAH,CAAL,GAAa,YAAjB,EAA2B;AAAA;AAA3B,GACI,CAFN,C,CAeI,EAXO,QAAA,CAAA,Y,kBAAX,YAAA,CAIM,KAJN,EAIM,WAJN,EAIM,CAHJ,YAAA,CAEc,sBAFd,EAEc;AAFM,IAAA,EAAE,EAAA,WAAa,QAAA,CAAA,YAAA,CAAa;AAElC,GAFd,E;qBACE;AAAA,aAAyB,CAAzB,WAAyB,CAAzB;AAAA,K;;;;GADF,E;;AAAA,I,MAAA,CAGI,CAJN,C,qCAWI,EALO,QAAA,CAAA,Q,kBAAX,YAAA,CAIM,KAJN,EAIM,WAJN,EAIM,CAHJ,YAAA,CAEc,sBAFd,EAEc;AAFM,IAAA,EAAE,EAAA,WAAa,QAAA,CAAA,QAAA,CAAS;AAE9B,GAFd,E;qBACE;AAAA,aAAqB,CAArB,WAAqB,CAArB;AAAA,K;;;;GADF,E;;AAAA,I,MAAA,CAGI,CAJN,C,qCAKI,CA7DN,C","sourcesContent":["<template>\n  <div id=\"post-page\">\n    <div v-if=\"post\">\n      <h1>Post: {{ id }}</h1>\n      <div class=\"title\">{{ post.title }}</div>\n\n      <div class=\"content\">{{ post.content }}</div>\n      <div v-if=\"updated\">\n        <label for=\"title\">Title</label>\n        <input type=\"text\" v-model=\"post.title\" id=\"title\" />\n        <div v-if=\"errors && errors.title\" class=\"error\">Title required</div>\n\n        <label for=\"favorite\" class=\"form-checkbox-label\">\n          <input type=\"checkbox\" v-model=\"post.favorite\" id=\"favorite\" />\n          Favorite?\n        </label>\n\n        <label for=\"featured\" class=\"form-checkbox-label\">\n          <input type=\"checkbox\" v-model=\"post.featured\" id=\"featured\" />\n          Featured?\n        </label>\n\n        <label for=\"content\">Content</label>\n        <textarea v-model=\"post.content\" id=\"content\"></textarea>\n        <div v-if=\"errors && errors.content\" class=\"error\">\n          Content required\n        </div>\n\n        <label for=\"categories\">Categories (separated by comma)</label>\n        <input type=\"text\" v-model=\"post.categories\" id=\"categories\" />\n      </div>\n\n      <button v-if=\"!updated\" v-on:click=\"updated = true\">Update</button>\n      <button v-if=\"updated\" v-on:click=\"update\">Update</button>\n      <button\n        v-if=\"updated\"\n        v-on:click=\"\n          updated = false;\n          success = false;\n          failure = false;\n        \"\n      >\n        Cancel\n      </button>\n      <div v-if=\"success\" class=\"success\">Post successfully updated</div>\n      <div v-if=\"failure && errors\" class=\"error\">Error updating post</div>\n    </div>\n    <div v-else>\n      <h2>Post {{ id }} not found</h2>\n    </div>\n\n    <div v-if=\"previousPost\">\n      <router-link v-bind:to=\"'/post/' + previousPost.id\">\n        <button>Previous</button>\n      </router-link>\n    </div>\n\n    <div v-if=\"nextPost\">\n      <router-link v-bind:to=\"'/post/' + nextPost.id\">\n        <button>Next</button>\n      </router-link>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { axios } from \"@/common/app.js\";\n\nexport default {\n  props: {\n    id: {\n      type: String,\n    },\n    posts: {\n      type: Array,\n      default: null,\n    },\n  },\n  data() {\n    return {\n      updated: false,\n      failure: false,\n      success: false,\n      errors: [],\n    };\n  },\n  methods: {\n    update() {\n      var definedPost = this.post;\n      if (definedPost.favorite) {\n        definedPost.favorite = 1;\n      } else {\n        definedPost.favorite = 0;\n      }\n      if (definedPost.featured) {\n        definedPost.featured = 1;\n      } else {\n        definedPost.featured = 0;\n      }\n      axios.put(\"/post/\" + this.post.id, definedPost).then((response) => {\n        if (response.data && response.data.errors) {\n          this.failure = true;\n          this.success = false;\n          // continue from here with errors\n          this.errors = response.data.errors;\n        } else {\n          this.errors = null;\n          this.success = true;\n          this.failure = false;\n          this.$emit(\"update-posts\");\n        }\n      });\n    },\n  },\n  computed: {\n    nextPost() {\n      var posted;\n      if (this.posts && this.post) {\n        var id = this.post.id;\n        this.posts.forEach(function (post, index, posts) {\n          if (id == post.id) {\n            if (posts[index + 1]) {\n              posted = posts[index + 1];\n            } else if (posts.length > 1) {\n              posted = posts[0];\n            }\n          }\n        });\n      }\n      return posted;\n    },\n    previousPost() {\n      var posted;\n      if (this.posts && this.post) {\n        var id = this.post.id;\n        this.posts.forEach(function (post, index, posts) {\n          if (id == post.id) {\n            if (posts[index - 1]) {\n              posted = posts[index - 1];\n            } else if (posts.length > 1) {\n              posted = posts[posts.length - 1];\n            }\n          }\n        });\n      }\n      return posted;\n    },\n    post() {\n      if (this.posts) {\n        var post;\n        var id = this.id;\n        this.posts.forEach(function (producted) {\n          if (producted.id == id) {\n            post = producted;\n          }\n        });\n        if (post) {\n          if (Number(post.favorite) == 1) {\n            post.favorite = true;\n          } else {\n            post.favorite = false;\n          }\n          if (Number(post.featured) == 1) {\n            post.featured = true;\n          } else {\n            post.featured = false;\n          }\n        }\n        return post;\n      } else {\n        return null;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.error {\n  color: red;\n}\n\n.success {\n  color: green;\n}\n\n#inputs {\n  text-align: left;\n}\n\ninput,\nlabel,\ntextarea,\nbutton {\n  display: block;\n  float: left;\n  clear: both;\n}\n\n#product-page {\n  border: 1px solid var(--silver);\n  text-align: center;\n  padding: 15px;\n  margin-left: auto;\n  margin-right: auto;\n  width: 30%;\n  min-width: 300px;\n}\n\n.title {\n  height: 50px;\n  font-size: 2rem;\n  margin: 5px 0 10px 0;\n  vertical-align: baseline;\n}\n\n.content {\n  margin: auto;\n  text-align: left;\n  font-style: italic;\n  line-height: 1.5;\n}\n</style>"],"sourceRoot":""}]}